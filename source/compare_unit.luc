module compare_unit (
 
    // inputs
    input signal[6],
    input n,
    input v,
    input z,
    //outputs
    output cout[16]
  ) {
  
  always {
    cout = 16b0;
    case (signal) {
      
      b00: cout[0] = 0;
      b01: cout[0] = z;
      b10: cout[0] = n ^ v;
      b11: cout[0] = z | (n ^ v);
    } 
  }
}
